{
  "name": "png-ts",
  "version": "0.1.1",
  "description": "A PNG decoder written in TypeScript",
  "main": "lib/png.js",
  "unpkg": "dist/pdf-lib.min.js",
  "module": "es/index.js",
  "types": "lib/png.d.ts",
  "scripts": {
    "build-lib": "tsc --module CommonJS --outDir lib",
    "build-es": "tsc --module ES2015 --outDir es",
    "build-umd": "rollup -c rollup.config.js -o dist/pdf-lib.js",
    "build-umd-min": "UGLIFY=true rollup -c rollup.config.js -o dist/pdf-lib.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devongovett/png.js.git"
  },
  "author": "Andrew Dillon <andrew.dillon.j@gmail.com>",
  "contributors": [
    "Devon Govett <devongovett@gmail.com> (http://badassjs.com/)"
  ],
  "bugs": "https://github.com/Hopding/png.js/issues",
  "devDependencies": {
    "@types/pako": "^1.0.0",
    "prettier": "^1.15.3",
    "rollup": "^0.68.0",
    "rollup-plugin-analyzer": "^2.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "pako": "^1.0.6"
  }
}
